{
  "html": {
    "abbreviations": {
      "link": "<link rel=\"${1:stylesheet}\" href=\"$2\" />",
      "a": "<a href=\"\">",
      "template": "<template />",
      "tmp": "<template />"
    },
    "snippets": {
      "link": "<link rel=\"${1:stylesheet}\" href=\"$2\" />",
      "js": "<script>${1}</script>",
      "css": "<style>$1</style>",
      "tmp": "<template>$1</template>",
      "email": "<input type=\"email\" minlength=\"6\" maxlength=\"320\" pattern=\"(?=^\\\\w[\\\\w_.+-]{0,63}@\\\\w[\\\\w_.-]{0,251}\\.[a-zA-Z]{2,8}$)^\\\\w([\\\\w_.+-]{0,1})(\\\\w{1,31}[\\\\w_.+-]{0,1}\\\\w{1,31}){1,21}([_.+-]\\\\w+){0,10}@((\\\\w|\\\\w[\\\\w_.-]\\\\w){1,125}\\.(\\\\w|\\\\w[\\\\w_.-]\\\\w){1,125}){1,85}$\"$1>",
      "pw": "<input type=\"password\" minlength=\"${1:6}\" maxlength=\"320\"$2>",
      "file": "<input type=\"file\" $1 />"
    }
  },
  "css": {
    "snippets": {
      "mx"     : "margin-left   : ${1:auto};\nmargin-right  : ${1:auto};$2",
      "my"     : "margin-top    : ${1:auto};\nmargin-bottom : ${1:auto};$2",
      "ma"     : "margin-top    : ${1:auto};\nmargin-left   : ${3:auto};\nmargin-bottom : ${2:auto};\nmargin-right  : ${4:auto};$5",
      "mil"    : "margin-inline:|;",
      "mils"   : "margin-inline-start:|;",
      "mile"   : "margin-inline-end:|;",
      "mbl"    : "margin-block:|;",
      "mbls"   : "margin-block-start:|;",
      "mble"   : "margin-block-end:|;",
      "box"    : "height: $1;\nwidth: $1;$2",
      "mnbox"  : "min-height:$1\nmin-width:$1;$2",
      "mxbox"  : "max-height:$1\nmax-width:$1;$2",
      "ilz"    : "inline-size:|;",
      "mnilz"  : "min-inline-size:|;",
      "mxilz"  : "max-inline-size:|;",
      "blz"    : "block-size:|;",
      "mnblz"  : "min-block-size:|;",
      "mxblz"  : "max-block-size:|;",
      "mxw"    : "max-width: ${1:100%};\nwidth: ${2:auto};$3",
      "mnw"    : "min-width: ${1:100%};\nwidth: ${2:auto};$3",
      "cw"     : "min-width: ${1:auto};\nmax-width: ${2:100%};\nwidth: ${3:auto};$4",
      "ch"     : "min-height: ${1:auto};\nmax-height: ${2:100%};\nheight: ${3:auto};$4",
      "mxh"    : "max-height: ${1:100%};\nheight: ${2:auto};$3",
      "mnh"    : "min-height: ${1:100%};\nheight: ${2:auto};$3",
      "px"     : "padding-left: ${1:auto};\npadding-right: ${1:auto};$2",
      "py"     : "padding-top: ${1:auto};\npadding-bottom: ${1:auto};$2",
      "pil"    : "padding-inline:|;",
      "pils"   : "padding-inline-start:|;",
      "pile"   : "padding-inline-end:|;",
      "pbl"    : "padding-block:|;",
      "pbls"   : "padding-block-start:|;",
      "pble"   : "padding-block-end:|;",
      "bdx"    : "border-left: ${1:1px} ${2:solid} ${3:#000};\nborder-right: ${1:1px} ${2:solid} ${3:#000};$4",
      "bdy"    : "border-top: ${1:1px} ${2:solid} ${3:#000};\nborder-bottom: ${1:1px} ${2:solid} ${3:#000};$4",
      "bdil"   : "border-inline:|;",
      "bdils"  : "border-inline-start:|;$0",
      "bdile"  : "border-inline-end:|;",
      "bdbl"   : "border-block:|;",
      "bdbls"  : "border-block-start:|;",
      "bdble"  : "border-block-end:|;",
      "bdwx"   : "border-left-width: $1;\nborder-right-width: $1;$2",
      "bdwy"   : "border-top-width: $1;\nborder-bottom-width: $1;$2",
      "bdwil"  : "border-inline-width:|;",
      "bdwils" : "border-inline-start-widht:|;",
      "bdwile" : "border-inline-end-widht:|;",
      "bdwbl"  : "border-block-width:|;",
      "bdwbls" : "border-block-start-width:|;",
      "bdwble" : "border-block-end-width:|;",
      "bdsx"   : "border-left-style: ${1:solid};\nborder-right-style: ${1:solid};$2",
      "bdsy"   : "border-top-style: ${1:solid};\nborder-bottom-style: ${1:solid};$2",
      "bdsil"  : "border-inline-style:|;",
      "bdsils" : "border-inline-start-style:|;",
      "bdsile" : "border-inline-end-style:|;",
      "bdsbl"  : "border-block-style:|;",
      "bdsbls" : "border-block-start-style:|;",
      "bdsble" : "border-block-end-style:|;",
      "bdcx"   : "border-left-color: ${1:#000};\nborder-right-color: ${1:#000};$2",
      "bdcy"   : "border-top-color: ${1:#000};\nborder-bottom-color: ${1:#000};$2",
      "bdcil"  : "border-inline-color:|;",
      "bdcils" : "border-inline-start-color:|;",
      "bdcile" : "border-inline-end-color:|;",
      "bdcbl"  : "border-block-color:|;",
      "bdcbls" : "border-block-start-color:|;",
      "bdcble" : "border-block-end-color:|;",
      "sbtn": "cursor: pointer;\ndisplay: ${1:inline-block};\nbackground-color: ${2:transparent};\ncolor: ${3:inherit};\nvertical-align: ${4:middle};\nborder: ${5:none};$6"
    }
  }
}
