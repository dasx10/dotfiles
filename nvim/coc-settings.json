{
  "codeLens.enable": true,
  "javascript.suggestionActions.enabled": false,
  "suggest.triggerAfterInsertEnter": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "workspace.workspaceFolderCheckCwd": true,
  "suggest.noselect": false,
  "suggest.snippetIndicator": "       ∷",
  "html.suggest.html5": true,
  "suggest.labelMaxLength": 70,
  "suggest.completionItemKindLabels": {
    "class"         : "",
    "color"         : "",
    "constant"      : "",
    "constructor"   : "",
    "default"       : "",
    "enum"          : "",
    "enumMember"    : "",
    "event"         : "鬒",
    "field"         : "料",
    "file"          : "",
    "folder"        : "",
    "function"      : "ƒ",
    "interface"     : "",
    "keyword"       : "",
    "method"        : "",
    "module"        : "",
    "operator"      : "Ψ",
    "property"      : "",
    "reference"     : "渚",
    "snippet"       : "",
    "struct"        : "פּ",
    "text"          : "",
    "typeParameter" : "",
    "unit"          : "",
    "value"         : "",
    "variable"      : "",
  },
  "workspace.bottomUpFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "vue"
  ],
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "snippets.textmateSnippetsRoots": ["~/.config/coc.snippets"],
  "snippets.extends": {
    "vue"             : ["javascript", "css"],
    "javascriptreact" : ["javascript"],
    "typescriptreact" : ["typescript"]
  },
  "languageserver": {},
  "emmet.includeLanguages": {
    "vue-html"    : "html",
    "vue"         : "javascript",
    "vue-postcss" : "css",
    "javascript"  : "css",
    "hbs"         : "html"
  },
  "emmet.syntaxProfiles": {
    "vue": "css"
  },
  "emmet.optimizeStylesheetParsing" : true,
  "emmet.extensionsPath"            : "~/.config/nvim/",
  "snippets.userSnippetsDirectory"  : "~/.config/nvim/snippets",
  "explorer.icon.enableNerdfont"    : true,
  "explorer.width"                  : 30,
  "explorer.trash.command"          : "nodejs:module",
  "explorer.file.hiddenRules"       : {
    "filenames": ["node_modules", ".git", "package-lock.json", "yarn.lock"]
  },
  "snippets.ultisnips.pythonPrompt": false,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.locale": "ru"
}
