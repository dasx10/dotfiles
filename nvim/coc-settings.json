{
  "codeLens.enable": true,
  "javascript.suggestionActions.enabled": false,
  "suggest.triggerAfterInsertEnter": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.displayByAle": false,
  "workspace.workspaceFolderCheckCwd": true,
  "suggest.noselect": false,
  "suggest.snippetIndicator": "       ∷",
  "html.suggest.html5": true,
  "suggest.labelMaxLength": 70,
  "suggest.completionItemKindLabels": {
    "class"         : "",
    "color"         : "",
    "constant"      : "",
    "constructor"   : "",
    "default"       : "",
    "enum"          : "",
    "enumMember"    : "",
    "event"         : "鬒",
    "field"         : "料",
    "file"          : "",
    "folder"        : "",
    "function"      : "ƒ",
    "interface"     : "",
    "keyword"       : "",
    "method"        : "",
    "module"        : "",
    "operator"      : "Ψ",
    "property"      : "",
    "reference"     : "渚",
    "snippet"       : "",
    "struct"        : "פּ",
    "text"          : "",
    "typeParameter" : "",
    "unit"          : "",
    "value"         : "",
    "variable"      : "",
  },
  "workspace.bottomUpFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "snippets.textmateSnippetsRoots": ["~/.config/coc.snippets"],
  "snippets.extends": {
    "vue"             : ["javascript", "css"],
    "javascriptreact" : ["javascript"],
    "typescriptreact" : ["typescript"]
  },
  "languageserver": {
		"terraform": {
			"command": "terraform-ls",
			"args": ["serve"],
			"filetypes": [
				"terraform",
				"tf"
			],
			"initializationOptions": {},
			"settings": {}
		},
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  },
  "yaml.schemas": {
    "kubernetes": "/*.yaml"
  },
  "emmet.includeLanguages": {
    "hbs"         : "html"
  },
  "emmet.optimizeStylesheetParsing" : true,
  "emmet.extensionsPath"            : "~/.config/nvim/",
  "snippets.userSnippetsDirectory"  : "~/.config/nvim/snippets",
  "snippets.ultisnips.pythonPrompt": false,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.locale": "ru",
  "files.associations": {
    "*.tf.hbs": "terraform",
    "*.yaml.hbs": "yaml",
    "*.yml.hbs": "yaml"
  }
}
